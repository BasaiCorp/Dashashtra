    <script>
        // Check for redirection
        if (window.location.search.includes('redirected=true')) {
            const alertBox = document.createElement('div');
            alertBox.className = 'alert alert-info alert-dismissible fade show';
            alertBox.setAttribute('role', 'alert');
            alertBox.innerHTML = `
                You've been redirected to the homepage. Please login to continue.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            `;
            document.body.insertBefore(alertBox, document.body.firstChild);
        }

        // Create particles
        document.addEventListener('DOMContentLoaded', function() {
            const container = document.querySelector('.floating-particles');
            
            // Clear existing particles
            container.innerHTML = '';
            
            // Create new particles
            for (let i = 0; i < 30; i++) {
                const particle = document.createElement('div');
                particle.className = 'particle';
                
                // Set random styles
                particle.style.left = (Math.random() * 100) + '%';
                particle.style.top = (Math.random() * 100) + '%';
                particle.style.width = (Math.random() * 15 + 3) + 'px';
                particle.style.height = (Math.random() * 15 + 3) + 'px';
                particle.style.animationDuration = (Math.random() * 15 + 10) + 's';
                particle.style.animationDelay = (Math.random() * 5) + 's';
                
                container.appendChild(particle);
            }
            
            // Navbar scroll effect
            const navbar = document.getElementById('mainNav');
            
            window.addEventListener('scroll', function() {
                if (window.scrollY > 50) {
                    navbar.classList.add('scrolled');
                } else {
                    navbar.classList.remove('scrolled');
                }
            });
            
            // Apply rainbow color effect to elements
            function updateRainbowElements() {
                // Get the current primary color from CSS variable
                const primaryColor = getComputedStyle(document.documentElement).getPropertyValue('--primary-color').trim();
                
                // Update elements that need the current primary color
                document.querySelectorAll('.particle').forEach(particle => {
                    particle.style.backgroundColor = primaryColor;
                    particle.style.opacity = '0.1';
                });
                
                document.querySelectorAll('.glow').forEach(glow => {
                    glow.style.background = `radial-gradient(circle, ${primaryColor}, transparent 70%)`;
                    glow.style.opacity = '0.2';
                });
                
                // Request next animation frame
                requestAnimationFrame(updateRainbowElements);
            }
            
            // Start the animation loop
            updateRainbowElements();
        });
    </script>
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
